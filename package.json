  "repository": {
    "type": "git",
    "url": "https://github.com/aaronbeall/convert-case-ts.git"
  },
  "bugs": {
    "url": "https://github.com/aaronbeall/convert-case-ts/issues"
  },
  "homepage": "https://github.com/aaronbeall/convert-case-ts#readme",
