{
  "name": "convert-case-ts",
  "version": "1.0.0",
  "description": "Type-safe string case conversion library for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
